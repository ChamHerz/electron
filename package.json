{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "public/electron.js",
  "author": "Denis Adriel Chambi",
  "homepage": "./",
  "scripts": {
    "server-dev-start": "nodemon src/server/index.js",
    "react-dev-start": "cross-env BROWSER=none yarn react-scripts start",
    "electron-dev-start": "wait-on http://localhost:3000 && electron .",
    "dev-start": "concurrently --kill-others \"yarn server-dev-start\" \"yarn react-dev-start\" \"yarn electron-dev-start\"",
    "dev-react-start": "concurrently --kill-others \"yarn react-dev-start\" \"yarn electron-dev-start\"",
    "build": "react-scripts build",
    "electron-pack": "electron-builder -c.extraMetadata.main=build/electron.js",
    "preelectron-pack": "yarn build"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "license": "CC0-1.0",
  "devDependencies": {
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "electron": "^9.1.1",
    "electron-builder": "^22.7.0",
    "electron-devtools-installer": "^3.1.1",
    "prettier": "^2.0.5",
    "wait-on": "^5.1.0"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "electron-is-dev": "^1.2.0",
    "express": "^4.17.1",
    "nodemon": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "sequelize": "^6.3.3",
    "sqlite3": "^5.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "com.electron",
    "files": [
      "build/**/*",
      "src/server/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
